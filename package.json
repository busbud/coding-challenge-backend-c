{
  "name": "coding-challenge-backend-c",
  "version": "0.0.0",
  "description": "coding challenge in Nodejs",
  "main": "app.js",
  "scripts": {
    "start": "node src/index.ts",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "jest --watchAll --no-cache",
    "deploy": "./scripts/deploy.sh",
