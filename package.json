{
  "name": "coding-challenge-backend-c",
  "version": "0.0.0",
  "description": "coding challenge in Nodejs",
  "main": "app.js",
  "bin": {
    "es-load-index": "src/services/es_search/bin/load_and_create_index.js"
  },
  "scripts": {
    "start": "node lib/app.js",
    "start:dev": "nodemon --exec babel-node src/app.js",
    "test": "mocha -r @babel/register ./test/suggestions.js",
    "test:unit": "mocha -r @babel/register --ignore ./test/suggestions.js ./test/**/*.js ",
    "test:es": "cross-env NOCK_DISABLE=true mocha -r @babel/register ./test/services/SearchCityFromEs.js",
    "build": "babel src -d lib/",
    "lintjs": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" ./",
    "lintjs:fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" ./ --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" && yarn lintjs-fix"
  },
  "author": "",
  "license": "WTFPL",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "chai": "^1.9.1",
    "chai-almost": "^1.0.1",
    "chai-sorted": "^0.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.1",
    "cross-env": "^7.0.2",
    "d3-scale": "^3.2.1",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.4",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": "12.16.1"
  }
}
