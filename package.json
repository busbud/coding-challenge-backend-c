{
  "name": "coding-challenge-backend-c",
  "version": "0.0.1",
  "description": "coding challenge in Nodejs",
  "main": "app.js",
  "engines": {
    "node": "16.14.0",
    "npm": "8.12.1"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "api:build": "npm install && npm run build",
    "start": "npm run build && node --experimental-specifier-resolution=node src/dist/app.js",
    "lint": "prettier --write .",
    "watch": "cd ./src && tsc-watch --onsuccess \"node --experimental-specifier-resolution=node dist/app.js\"",
    "test": "mocha  --timeout 10000  --exit --require ./node_modules/ts-node/register 'test/**/*.ts'"
  },
  "author": "",
  "license": "WTFPL",
