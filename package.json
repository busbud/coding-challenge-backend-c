{
  "name": "busbud-challenge",
  "version": "0.1.0",
  "description": "Busbud Coding Challenge in Node.js",
  "author": "Matthieu Symoens",
  "license": "WTFPL",
  "scripts": {
    "dev": "NODE_PATH=src ts-node-dev src",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "NODE_PATH=src jest --verbose",
    "test:coverage": "NODE_PATH=src jest --coverage",
    "migration": "NODE_PATH=src ts-node src/command/migration run",
    "migration:up": "NODE_PATH=src ts-node src/command/migration up",
    "migration:down": "NODE_PATH=src ts-node src/command/migration down",
    "migration:reset": "NODE_PATH=src ts-node src/command/migration reset",
    "migration:list": "NODE_PATH=src ts-node src/command/migration list",
    "migration:make": "NODE_PATH=src ts-node src/command/migration make",
    "seed": "NODE_PATH=src ts-node src/command/seed run",
    "seed:make": "NODE_PATH=src ts-node src/command/seed make",
    "build": "tsc",
    "deploy:migration": "NODE_PATH=dist node dist/command/migration run",
    "deploy:seed": "NODE_PATH=dist node dist/command/seed run",
    "start": "NODE_PATH=dist node dist"
  },
  "engines": {
    "node": "18.1"
  },
  "dependencies": {
    "csv-parse": "^5.0.4",
    "diacritics": "^1.3.0",
    "dotenv": "^16.0.0",
    "haversine": "^1.1.1",
    "ioredis": "^5.0.4",
    "joi": "^17.6.0",
    "knex": "^2.0.0",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa2-swagger-ui": "^5.5.1",
    "object-hash": "^3.0.0",
    "pg": "^8.7.3",
    "swagger-jsdoc": "^6.2.1"
  },
  "devDependencies": {
    "@types/diacritics": "^1.3.1",
    "@types/haversine": "^1.1.5",
    "@types/jest": "^27.5.0",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.31",
    "@types/object-hash": "^2.2.1",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^28.1.0",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
