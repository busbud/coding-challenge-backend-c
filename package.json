    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.8.0",
    "pg": "^8.5.1",
    "ts-node-dev": "^1.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.19",
    "@types/supertest": "^2.0.10",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
