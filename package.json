    "@types/string-similarity": "^4.0.0",
    "axios": "^1.2.3",
    "body-parser": "^1.20.1",
    "chai": "^1.9.1",
    "d3-dsv": "^3.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
