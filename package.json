{
  "name": "coding-challenge-backend-c",
  "version": "0.0.0",
  "description": "coding challenge in Nodejs",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/busbud/coding-challenge-backend-c.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "eslint-check": "./node_modules/.bin/eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "reload": "nodemon ./app.js",
    "start:dev": "npm-run-all --parallel reload dev",
    "start": "node app.js",
    "unit-test": "NODE_ENV=test nyc ./node_modules/mocha/bin/mocha --require @babel/register ./src/**/*.test.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --require @babel/register ./test/",
    "format": "prettier-eslint \"src/**/*.js\"",
    "publish": ""
  },
  "author": "",
  "license": "WTFPL",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "pg-pool": "^2.0.3",
    "pg-query-stream": "^1.1.1",
    "readable-stream": "^2.3.3",
    "utf8": "^2.1.2"
  },
  "engines": {
    "node": "8.5.0",
    "npm": "5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.32",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/plugin-syntax-async-generators": "^7.0.0-beta.32",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-beta.32",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.32",
    "@babel/plugin-transform-regenerator": "^7.0.0-beta.32",
    "@babel/polyfill": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "babel-eslint": "^8.0.2",
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "prettier": "^1.8.2",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.4.0",
    "regenerator-runtime": "^0.11.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sequelize-cli": "^3.1.0",
    "supertest": "^3.0.0"
  }
}
