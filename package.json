{
  "name": "coding-challenge-backend-c",
  "version": "1.0.0",
  "description": "coding challenge in Nodejs accepted by Hama MOHAMED MAHMOUD",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "dev:start": "npm-run-all build start",
    "start": "node .",
    "build": "tsc",
    "lint": "eslint -c .eslintrc.json --fix './**/*.{ts,tsx}'",
    "test": "./node_modules/mocha/bin/mocha ./test/",
    "data": "node ./dist/app/dev/data-loader.js",
    "create_db": "sequelize-cli db:create || true",
    "drop_db": "sequelize-cli db:drop || true",
    "generate_migration": "sequelize-cli migration:generate --name",
    "migrate": "sequelize-cli db:migrate",
    "pre:test": "NODE_ENV=test npm-run-all create_db migrate load_data",
    "load_data": "npm-run-all build data"
  },
  "author": "Hama MOHAMED MAHMOUD",
  "license": "WTFPL",
  "dependencies": {
    "body-parser": "^1.20.0",
    "celebrate": "^15.0.1",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "mocha": "^9.2.2",
    "papaparse": "^5.3.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "supertest": "^6.2.2"
  },
  "engines": {
    "node": "16.14.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.24",
    "@types/papaparse": "^5.3.2",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.6.3"
  }
}
