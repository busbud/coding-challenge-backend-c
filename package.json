{
  "name": "coding-challenge-backend-c",
  "version": "0.0.1",
  "description": "coding challenge in Nodejs",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js ./src/app.js",
    "debug": "./node_modules/nodemon/bin/nodemon.js --legacy-watch --watch ./src --inspect=0.0.0.0:5858 --nolazy ./src/app.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "./node_modules/mocha/bin/mocha --recursive test/unit/**/*.js",
    "test:integration": "export PORT=5432; ./node_modules/mocha/bin/mocha --recursive test/integration/**/*.js",
    "coverage": "npm run coverage:unit && npm run coverage:integration && npm run coverage:report",
    "coverage:unit": "nyc --silent npm run test:unit",
    "coverage:integration": "nyc --silent --no-clean npm run test:integration",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "ci_lint": "yaml-validator .travis.yml"
  },
  "author": "",
  "license": "WTFPL",
  "dependencies": {
    "csv-parser": "^2.3.3"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "mocha": "^8.2.0",
    "nock": "^13.0.4",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.1",
    "supertest": "^5.0.0",
    "yaml-validator": "^3.0.0"
  },
  "engines": {
    "node": "12.18.4"
  }
}
